{
  "name": "express-ts",
  "version": "0.0.1",
  "main": "src/app.ts",
  "license": "MIT",
  "author": "MeeMeChu",
  "scripts": {
    "start": "node -r dotenv/config dist/app.js",
    "build": "tsc -p ./tsconfig.json && tsc-alias -p ./tsconfig.json",
    "dev": "concurrently -k \"tsc -p ./tsconfig.json -w\" \"nodemon -r dotenv/config -r @babel/register -r tsconfig-paths/register src/app.ts\""
  },
  "dependencies": {
    "config": "^3.3.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/register": "^7.25.9",
    "@types/config": "^3.3.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.4",
    "babel-plugin-module-resolver": "^5.0.2",
    "concurrently": "^9.1.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "nodemon": "^3.1.9",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  }
}
